@import "/wxSearch/wxSearch.wxss";

.container {
    background-color: #F2f2f2;
    
}

.line_flag {
    width: 5px;
    height: 100%;
    height: 20px;
    display: inline-block;
    background-color: #f9f027;
}

view.text {
    display: flex;
    align-items: center;
    position: relative;
    padding: 10px 10px 10px 12px;
    font-size: 15px;
    color: #656565;
}


.venues_box {
    padding: 0 10rpx;
}

.venues_item {
    display: inline-block;
    width: 49%;
    margin: 0 0.5%;
}

.venues_item image{
    width: 100%;
    height: 90px;
}